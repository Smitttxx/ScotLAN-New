{
  "name": "scotlan",
  "version": "0.1.0",
  "private": false,
  "dependencies": {
    "aws-amplify": "^1.1.3",
    "google-map-react": "^1.1.2",
    "graphql": "^14.0.2",
    "polyfill": "^0.1.0",
    "react": "^16.5.2",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.2",
    "react-ga": "^2.5.6",
    "react-iframe": "^1.3.0",
    "react-router-bootstrap": "^0.24.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "react-stripe-checkout": "^2.6.3",
    "react-stripe-elements": "^2.0.3",
    "react-tabs": "^3.0.0",
    "react-tippy": "^1.2.3",
    "sweetalert2": "^7.18.0",
    "sweetalert2-react-content": "^1.0.1"
  },
  "scripts": {
    "start": "set REACT_APP_STAGE=dev && react-scripts start",
    "build": "react-scripts build",
    "predeploy": "set REACT_APP_STAGE=dev && npm run build",
    "deploy": "aws s3 sync build/ s3://scotlan --acl public-read",
    "postdeploy": "aws cloudfront create-invalidation --distribution-id ELJ4B0DFSQZJP --paths /*",
    "predeploy:prod": "set REACT_APP_STAGE=production && npm run build",
    "deploy:prod": "aws s3 sync build/ s3://scotlanproduction --acl public-read",
    "postdeploy:prod": "aws cloudfront create-invalidation --distribution-id E1D6E5W8DM785D --paths /*",
    "eject": "react-scripts eject"
  }
}
